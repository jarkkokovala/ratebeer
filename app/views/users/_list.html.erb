<table class="table table-hover">
  <thead>
    <tr>
      <th>Username</th>
      <th>Number of ratings</th>
    </tr>
  </thead>

  <tbody>
    <% users.each do |user| %>
      <tr>
        <td><%= link_to user.username, user_path(user) %></td>
        <td><%= user.ratings.count() %></td>
      </tr>
    <% end %>
  </tbody>
</table>